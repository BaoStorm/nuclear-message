<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nm.nmoauth.UserMapper">
    <!-- 根据主键查询-->
    <select id="selectByUserName" resultType="com.nm.nmoauth.User" parameterType="java.lang.String" >
        select  *
        from user
        where user_name = #{userName}
    </select>
</mapper>